<% provide(:title, 'New client') %>
<div class="row">
  <div class="span10 offset1">
    <h1>New client</h1>
    <%= simple_form_for(@client) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= render 'client_fields', client: @client, f: f %>
      <%= render 'shared/forms/submit_button', f: f %>
    <% end %>
  </div>
</div>