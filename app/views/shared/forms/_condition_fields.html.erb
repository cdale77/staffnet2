<div class="row field" >
  <div class="span12">
    <div class="span3">
      <%= f.attribute_fields do |a| %>
        <%= a.attribute_select %>
      <% end %>
    </div>
    <div class="span3">
      <%= f.predicate_select %>
    </div>
    <div class="span3">
      <%= f.value_fields do |v| %>
        <%= v.text_field :value %>
      <% end %>
    </div>
    <div class="span1">
      <%= link_to "remove", '#', class: "remove_fields" %>
    </div>
  </div>
</div>