<% provide(:title, 'New supporter') %>
<div class="row">
  <div class="span10 offset1">
    <h1>New supporter</h1>
    <%= simple_form_for(@supporter) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= render partial: 'supporter_fields', locals: { supporter: @supporter, supporter_types: @supporter_types, f: f } %>
      <div class="row">
        <div class="span2">
          <%= f.button :submit, input_html: { class: 'btn btn-large btn-primary'} %>
        </div>
      </div>
    <% end %>
  </div>
</div>