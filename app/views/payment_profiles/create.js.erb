(function() {
  $("#new-payment-profile-fields").collapse("hide");
  $("#flash-ajax").html("<%= j render 'shared/forms/payment_profile_alert', cim_profile: @cim_profile %>");
  $("#flash-ajax").collapse("show");
  <% if @cim_profile.success  %>
    $("#payment-profiles").append("<%= j render 'shared/forms/payment_profile', payment_profile: @payment_profile, n: '4' %>");
  <% end %>
  $("#btn-new-payment-profile").show();
})();

